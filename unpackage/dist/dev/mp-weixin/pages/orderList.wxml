<view class="ol"><view class="olt"><block wx:for="{{tabs}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{[(activeTab==index)?'active':'']}}" bindtap="__e"><view class="oltext">{{''+item+''}}</view><view class="oltLine"></view></view></block></view><block wx:if="{{data_list.length>0}}"><view class="olc"><block wx:for="{{data_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="olci"><view class="d1"><view class="d1t1">{{'预约单号：'+item.orderSn+''}}</view><view class="d1t2"><block wx:if="{{item.status==-1}}"><text>已取消</text></block><block wx:if="{{item.status==0}}"><text>预定中</text></block><block wx:if="{{item.status==1}}"><text>待支付</text></block><block wx:if="{{item.status==2}}"><text>已支付</text></block><block wx:if="{{item.status==3}}"><text>已发货</text></block></view></view><view class="d2"><view class="d2r1"><view class="d2r2j1">{{'下单时间：'+item.createTime+''}}</view><view class="d2r2j2">{{'数量：'+item.num+'片'}}</view></view><view class="d2r2"><image class="d2r2f1" src="../static/img/m5.png"></image><view class="d2r2f2">{{item.receiverName}}</view><view class="d2r2f3">{{item.receiverPhone}}</view></view><view class="d2r3">{{''+item.receiverProvince+item.receiverCity+item.receiverRegion+item.receiverDetailAddress+''}}</view></view><view class="d3"><block wx:if="{{item.orderType==1}}"><view class="d3r1">VIP推广领取</view></block><block wx:else><view class="d3r1"><block wx:if="{{item.status==2||item.status==3}}"><text>{{"已支付：¥"+item.amount}}</text></block><block wx:else><text>{{"待支付：¥"+item.amount}}</text></block></view></block><view class="d3r2"><block wx:if="{{item.status==0}}"><view data-event-opts="{{[['tap',[['cancelOrder',['$0'],[[['data_list','',index,'id']]]]]]]}}" class="d3r2b1" bindtap="__e">取消</view></block><block wx:if="{{item.status==0||item.status==1}}"><view data-event-opts="{{[['tap',[['toPath',['/pages/editaddr?id='+item.id+'&from=orderList']]]]]}}" class="d3r2b2" bindtap="__e">修改地址</view></block><block wx:if="{{item.status==1}}"><view data-event-opts="{{[['tap',[['pay',['$0'],[[['data_list','',index,'id']]]]]]]}}" class="d3r2b3" bindtap="__e">去支付</view></block><block wx:if="{{item.orderType!=1}}"><view><block wx:if="{{item.status==-1||item.status==3}}"><view data-event-opts="{{[['tap',[['toPath',['/pages/detail?id='+item.productId]]]]]}}" class="d3r2b2" bindtap="__e">再次预定</view></block></view></block></view></view><block wx:if="{{item.status==-1}}"><view class="d4"><block wx:if="{{item.cancelType==1}}"><text>用户取消</text></block><block wx:else><block wx:if="{{item.cancelType==2}}"><text>{{"超过"+config.BIZ_ORDER_OVER_TIME+"小时未支付自动取消"}}</text></block><block wx:else><text>管理员取消</text></block></block></view></block><block wx:if="{{item.status==2&&item.preDeliveryTime}}"><view class="d4 d5">{{'预计'+item.preDeliveryTime+'前发货'}}</view></block><block wx:if="{{item.status==3}}"><view data-event-opts="{{[['tap',[['copy',['$0'],[[['data_list','',index,'deliverySn']]]]]]]}}" class="d4" bindtap="__e">{{''+item.deliveryCompany+'：'}}<block wx:if="{{item.deliverySn}}"><text>{{item.deliverySn}}</text></block><block wx:else><text>单号数据同步中</text></block><image class="rb2h2t3" src="../static/img/copy.png" mode="widthFix"></image></view></block></view></block><block wx:if="{{hasMore}}"><view data-event-opts="{{[['tap',[['getMore',['$event']]]]]}}" class="olcim" bindtap="__e"><view class="rcm1">加载更多</view><image class="rcm1img" src="../static/img/down.png" mode="widthFix"></image></view></block></view></block><block wx:else><view class="olc"><view class="olcEmpty"><image class="oe1" src="../static/img/nodata.png" mode="widthFix"></image><view class="oe2">- 你还没有这类信息哦 -</view></view></view></block></view>